<p id="notice"><%= notice %></p>

<h1>News</h1>

<table>
  <thead>
    <tr>
      <th>Tittle</th>
      <th>Url</th>
      <br>
      
      <th>Likes</th>
      <th>Content</th>
      <th>User</th>
      <th>Created at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @news.each do |news| %>
      <tr>
        <td><%= news.tittle %></td>
        <td><%= news.url %></td>
        <td><%= news.content %></td>
        <td><%= news.created_At %></td>
        <td><%= news.likes %></td>
        <td><%= news.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New News', new_news_path %>
